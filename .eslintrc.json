{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "double", { "allowTemplateLiterals": true }],
    "no-tabs": "error",
    "indent": ["error", 2],
    "no-unused-vars": 0,
    "no-console": 1
  },
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  }
}